extends tex

# Include standalone
snippet standalone "Inlcude a standalone image" b
\includestandalone[mode=tex]{${1:file}}${0}
endsnippet

snippet subfig "Include subfigure env"
\begin{subfigure}[${1:b}]{${2:1}\textsize}
  \includegraphics[]{}
  \caption{${3}}%
  \label{fig:${4}}
\end{subfigure}${0}
endsnippet


